{
  "name": "sse-shared-worker",
  "version": "1.0.0",
  "description": "Utilities for Server-Sent Events (SSE) and SharedWorker-based SSE fan-out in the browser.",
  "license": "MIT",
  "author": "Mohammad Garmabi",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-garmabi/sse-shared-worker"
  },
  "keywords": [
    "sse",
    "server-sent-events",
    "eventsource",
    "sharedworker",
    "worker"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "pack:dry": "npm pack --dry-run"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  }
}
